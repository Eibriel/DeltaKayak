; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Delta Kayak"
config/description="Navigate an open-world island in your kayak, avoid the giant boat stalking you, and venture deep into the unknown to find your missing pet."
run/main_scene="res://menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=90
boot_splash/show_image=false
config/icon="res://icon.png"

[audio]

general/text_to_speech=true

[autoload]

Global="*res://global.gd"
GlobalSteam="*res://steam.gd"
GamePlatform="*res://game_platform.gd"
VR="*res://vr.gd"

[debug]

shapes/paths/geometry_width=10.0

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/mode=3
window/size/window_width_override=1200
window/size/window_height_override=800
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dkdata_inspector/plugin.cfg", "res://addons/dkworld/plugin.cfg", "res://addons/godot_resource_groups/plugin.cfg")

[file_customization]

folder_colors={
"res://data_resources/": "orange",
"res://dkdata/": "red",
"res://interactives/": "green"
}

[godot_resource_groups]

auto_rebuild=true

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom="res://themes/default_theme.tres"
theme/default_font_antialiasing=0

